{
  "name": "reason-i18n-extractor",
  "version": "2.0.0",
  "description": "Extract translations from your Reason",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "bin": [
        "Upgrade"
      ]
    },
    "install": [
      "esy-installer reason-i18n-extractor.install",
      "esy-installer reason-i18n-extractor-tests.install"
    ]
  },
  "scripts": {
    "test": "esy x RunTests.exe",
    "doc": "esy dune build @doc"
  },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.4.0 < 3.5.0",
    "@opam/dune": "^1.9.3",
    "@opam/reason": ">= 3.4.0 < 3.5.0",
    "@reason-native/console": "*",
    "@reason-native/pastel": "*",
    "@reason-native/rely": "*",
    "ocaml": "~4.6.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "@opam/odoc": "*",
    "ocaml": "~4.6.0"
  }
}
